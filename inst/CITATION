year <- 2023
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{cmdlr}: Choice Modeling in R",
         author = c(person(c("Erlend", "Dancke"), "Sandorf")),
         year = year,
         note = note,
         url = "https://cmdlr.edsandorf.me")
